<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.kulturhusfx.controllers.AdminManageHallsController">
         <children>
            <Button layoutX="33.0" layoutY="25.0" mnemonicParsing="false" onAction="#backToAdminMainPageBtn" text="Tilbake" />
            <Label layoutX="33.0" layoutY="93.0" prefHeight="17.0" prefWidth="255.0" text="Oversikt over lokaler:">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
      <TableView fx:id="tableViewHalls" layoutX="33.0" layoutY="123.0" prefHeight="200.0" prefWidth="740.0">
        <columns>
          <TableColumn fx:id="nameColumn" onEditCommit="#editHallNameCellEvent" prefWidth="159.0" text="Navn" />
          <TableColumn fx:id="typeOfHallColumn" onEditCommit="#editHallTypeCellEvent" prefWidth="121.0" text="Type lokale" />
            <TableColumn fx:id="numberOfSeatsColumn" minWidth="0.0" onEditCommit="#editNumberOfSeatsCellEvent" prefWidth="148.0" text="Antall plasser" />
        </columns>
      </TableView>
      <Button fx:id="deleteHallBtn" layoutX="35.0" layoutY="353.0" mnemonicParsing="false" onAction="#deleteHallFromTableView" prefHeight="27.0" prefWidth="110.0" text="Slett sal" />
         </children>
      </AnchorPane>
